<tr>
	<td>
		<div class="text-muted font-semibold me-3 {{ required ? 'required' : '' }}">{{ label }}</div>
	</td>
	<td>
		<widget-auto-complete ng-model="{{ prefix ?? 'vm.data' }}.{{ name }}" table="{{ table }}" normal_search="{{ normal_search }}" data="{{ prefix ?? 'vm.data' }}.{{ data }}" {{ required ? 'required' : '' }} {{ jenis ?? '' }} ></widget-auto-complete>
	</td>
	<td>
		{{ (additional ?? '') | raw }}
	</td>
</tr>
